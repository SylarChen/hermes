<?xml version="1.0" encoding="UTF-8"?>
<entities do-package="com.ctrip.hermes.meta.dal.meta" gen="true">
	<entity name="meta">
		<query-defs>
			<query name="findLatest" type="SELECT">
				<statement><![CDATA[SELECT <FIELDS/>
        FROM <TABLE/>
        ORDER BY <FIELD name='id'/> DESC LIMIT 1]]></statement>
			</query>
		</query-defs>
	</entity>
	<entity name="schema">
		<query-defs>
			<query name="findLatestByName" type="SELECT">
				<param name="name" />
				<statement><![CDATA[SELECT <FIELDS/>
        FROM <TABLE/>
        WHERE <FIELD name='name'/> = ${name}
        ORDER BY <FIELD name='id'/> DESC LIMIT 1]]></statement>
			</query>
			<query name="findByName" type="SELECT" multiple="true">
				<param name="name" />
				<statement><![CDATA[SELECT <FIELDS/>
        FROM <TABLE/>
        WHERE <FIELD name='name'/> = ${name}]]></statement>
			</query>
		</query-defs>
	</entity>
</entities>

